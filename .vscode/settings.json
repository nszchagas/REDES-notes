{
  "pasteImage.basePath": "${currentFileDir}",
  "pasteImage.path": "${currentFileDir}/assets",
  "pasteImage.defaultName": "mmss",
  "pasteImage.insertPattern": "${imageSyntaxPrefix}.${imageFilePath}${imageSyntaxSuffix}\n",
  "cSpell.words": ["Anotacionais", "COCOMO", "Codeforces", "Ishikawa", "Mbaud"],
  "markdownlint.config": {
    "MD033": false,
    "MD045": false,
    "MD041": false,
    "MD024": false
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "python.formatting.provider": "none",
  "files.associations": {
    "*.sql": "sql",
    "a": "plaintext",
    "*.md": "markdown",
    "any": "cpp",
    "array": "cpp",
    "atomic": "cpp",
